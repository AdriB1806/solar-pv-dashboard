# Environment variables for live data example
# Copy this file to .env and fill in your values

# OpenWeatherMap API Key (get free at https://openweathermap.org/api)
OPENWEATHER_API_KEY=...

# Location coordinates (default: Munich, Germany)
LATITUDE=48.1351
LONGITUDE=11.5820

# Poll interval in seconds (default: 300 = 5 minutes)
POLL_INTERVAL=50
